CREATE TABLE LIVROS(

NOME_DO_LIVRO      VARCHAR(50),
NOME_DO_AUTOR      VARCHAR(20),
SEXO_DO_AUTOR      CHAR(1),
NUMERO_DE_PAGINAS  INT(4),
NOME_DA_EDITORA    VARCHAR(15),
VALOR_DO_LIVRO     INT(6),
ESTADO_DA_EDITORA  CHAR(2),
ANO_DE_PUBLICACAO  INT(4) );


